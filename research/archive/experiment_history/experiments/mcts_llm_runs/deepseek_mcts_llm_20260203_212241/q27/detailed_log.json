      "output_sql": "WITH filtered_sales AS (\n    SELECT ss_item_sk, ss_store_sk, ss_quantity, ss_list_price, ss_coupon_amt, ss_sales_price\n    FROM store_sales\n    WHERE ss_cdemo_sk IN (\n        SELECT cd_demo_sk\n        FROM customer_demographics\n        WHERE cd_gender = 'F'\n          AND cd_marital_status = 'D'\n          AND cd_education_status = 'Secondary'\n    )\n      AND ss_sold_date_sk IN (\n        SELECT d_date_sk\n        FROM date_dim\n        WHERE d_year = 1999\n    )\n),\nstore_item_aggregates AS (\n    SELECT i.i_item_id,\n           s.s_state,\n           AVG(fs.ss_quantity) AS agg1,\n           AVG(fs.ss_list_price) AS agg2,\n           AVG(fs.ss_coupon_amt) AS agg3,\n           AVG(fs.ss_sales_price) AS agg4\n    FROM filtered_sales fs\n    JOIN item i ON fs.ss_item_sk = i.i_item_sk\n    JOIN store s ON fs.ss_store_sk = s.s_store_sk\n    WHERE s.s_state IN ('MO','AL', 'MI', 'TN', 'LA', 'SC')\n    GROUP BY i.i_item_id, s.s_state\n)\nSELECT i_item_id,\n       s_state,\n       grouping(s_state) g_state,\n       agg1,\n       agg2,\n       agg3,\n       agg4\nFROM store_item_aggregates\nGROUP BY ROLLUP (i_item_id, s_state)\nORDER BY i_item_id, s_state\nLIMIT 100"
    },
