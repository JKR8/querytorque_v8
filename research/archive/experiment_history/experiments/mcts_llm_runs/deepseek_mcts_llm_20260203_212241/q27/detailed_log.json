      "reward": 0.0,
      "output_sql": "SELECT\n  i.i_item_id,\n  COALESCE(fd.state, 'ALL') AS s_state,\n  CASE WHEN fd.state IS NULL THEN 1 ELSE 0 END AS g_state,\n  AVG(CASE WHEN NOT fd.state IS NULL THEN fd.total_quantity / NULLIF(fd.cnt, 0) END) AS agg1,\n  AVG(CASE WHEN NOT fd.state IS NULL THEN fd.total_list_price / NULLIF(fd.cnt, 0) END) AS agg2,\n  AVG(CASE WHEN NOT fd.state IS NULL THEN fd.total_coupon_amt / NULLIF(fd.cnt, 0) END) AS agg3,\n  AVG(CASE WHEN NOT fd.state IS NULL THEN fd.total_sales_price / NULLIF(fd.cnt, 0) END) AS agg4\nFROM filtered_data AS fd\nJOIN item AS i\n  ON fd.ss_item_sk = i.i_item_sk\nGROUP BY\n  ROLLUP (\n    i.i_item_id,\n    fd.state\n  )\nUNION ALL\nSELECT\n  i.i_item_id,\n  'ALL' AS s_state,\n  1 AS g_state,\n  AVG(fd.item_quantity / NULLIF(fd.item_cnt, 0)) AS agg1,\n  AVG(fd.item_list_price / NULLIF(fd.item_cnt, 0)) AS agg2,\n  AVG(fd.item_coupon_amt / NULLIF(fd.item_cnt, 0)) AS agg3,\n  AVG(fd.item_sales_price / NULLIF(fd.item_cnt, 0)) AS agg4\nFROM filtered_data AS fd\nJOIN item AS i\n  ON fd.ss_item_sk = i.i_item_sk\nGROUP BY\n  i.i_item_id\nORDER BY\n  i_item_id,\n  s_state\nLIMIT 100"
